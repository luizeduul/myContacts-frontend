{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-unsafe-optional-chaining": "off",
      "no-nonoctal-decimal-escape": "off",
      "react/jsx-filename-extension":[
        1, {"extensions": [".js", ".jsx"]}
      ],
      "react/react-in-jsx-scope": "off",
      "import/prefer-default-export": "off"
    }
}
